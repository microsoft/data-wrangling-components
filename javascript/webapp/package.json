{
	"name": "@data-wrangling-components/webapp",
	"private": true,
	"version": "2.2.0",
	"main": "src/index.ts",
	"scripts": {
		"clean": "essex clean build dist lib node_modules",
		"build": "tsc && webpack --mode production && shx cp -r public/* build/",
		"start": "webpack-dev-server",
		"start:client": "yarn start",
		"build:client": "yarn build && yarn archive",
		"archive": "essex zip --baseDir build deploy.zip '**/*'",
		"archive:client": "yarn archive",
		"build_stats": "webpack --mode production --json > stats.json"
	},
