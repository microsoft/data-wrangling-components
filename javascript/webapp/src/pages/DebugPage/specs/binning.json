{
	"$schema": "../../../../../../schema/workflow.json",
	"name": "Binning",
	"description": "Shows how different binning strategies and clamping work.",
	"steps": [
		{
			"verb": "bin",
			"input": { "source": "data/products.csv" },
			"output": { "target": "bin-1" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "auto",
				"to": "Binned1"
			}
		},
		{
			"verb": "bin",
			"input": { "source": "data/products.csv" },
			"output": { "target": "bin-2" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed width",
				"to": "Binned2",
				"fixedwidth": 100000
			}
		},
		{
			"verb": "bin",
			"input": { "source": "data/products.csv" },
			"output": { "target": "bin-3" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed count",
				"to": "Binned3",
				"fixedcount": 100
			}
		},
		{
			"verb": "bin",
			"input": { "source": "data/products.csv" },
			"output": { "target": "bin-4" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed width",
				"to": "Binned4",
				"fixedwidth": 100000,
				"min": 20000,
				"max": 1000000
			}
		},
		{
			"verb": "bin",
			"input": { "source": "data/products.csv" },
			"output": { "target": "bin-5" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed width",
				"to": "Binned5",
				"fixedwidth": 100000,
				"min": 20000,
				"max": 1000000,
				"clamped": true
			}
		}
	]
}
