{
	"$schema": "../../../../../../schema/workflow.json",
	"name": "Categorical sparkbar",
	"description": "Aggregates months into an array to demonstrate categorical Sparkbar cells",
	"steps": [
		{
			"verb": "aggregate",
			"input": { "source": "data/stocks.csv" },
			"output": { "target": "categorical" },
			"args": {
				"groupby": "Symbol",
				"column": "Month",
				"operation": "array_agg",
				"to": "Categories"
			}
		}
	]
}
