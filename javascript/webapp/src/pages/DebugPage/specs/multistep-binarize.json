{
	"name": "Multi-criteria binarize",
	"description": "Use multiple criteria chained together to create a boolean condition OR for a single binarized output column.",
	"steps": [
		{
			"verb": "binarize",
			"input": "data/companies.csv",
			"output": "binarize-1",
			"args": {
				"column": "Employees",
				"to": "Smallest and Largest",
				"criteria": [
					{
						"operator": "<",
						"value": 150000,
						"type": "value"
					},
					{
						"operator": ">",
						"value": 200000,
						"type": "value"
					}
				],
				"logical": "or"
			}
		}
	]
}
